                        <m:mn>1</m:mn>
                    </m:mrow>
                </m:mfrac>
            </m:math>
            <m:math display="block">
                <m:mi>x</m:mi>
                <m:mo>=</m:mo>
                <m:mfrac>
                    <m:mrow>
                        <m:mi>a</m:mi>
                        <m:mo>+</m:mo>
                        <m:mi>b</m:mi>
                    </m:mrow>
                    <m:mi>c</m:mi>
                </m:mfrac>
            </m:math>
            <m:math display="block">
                <m:mfrac>
                    <m:mrow>
                        <m:mfenced>
                            <m:mrow><m:mi>a</m:mi>
                                <m:mo>+</m:mo>
                                <m:mi>b</m:mi></m:mrow>
                        </m:mfenced>
                        <m:mo>&sdot;</m:mo>
                        <m:mfenced>
                            <m:mrow><m:mi>a</m:mi>
                                <m:mo>-</m:mo>
                                <m:mi>b</m:mi></m:mrow>
                        </m:mfenced>
                    </m:mrow>
                    <m:mi>c</m:mi>
                </m:mfrac>
            </m:math>
            <m:math display="block">
                <m:mi>x</m:mi>
                <m:mo>=</m:mo>
                <m:mfrac>
                    <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mi>b</m:mi>
                        <m:mo>&PlusMinus;</m:mo>
                        <m:msqrt>
                            <m:msup>
                                <m:mi>b</m:mi>
                                <m:mn>2</m:mn>
                            </m:msup>
                            <m:mo>-</m:mo>
                            <m:mn>4</m:mn>
                            <m:mo>&InvisibleTimes;</m:mo>
                            <m:mi>a</m:mi>
                            <m:mo>&InvisibleTimes;</m:mo>
                            <m:mi>c</m:mi>
                        </m:msqrt>
                    </m:mrow>
                    <m:mrow>
                        <m:mn>2</m:mn>
                        <m:mo>&InvisibleTimes;</m:mo>
                        <m:mi>a</m:mi>
                    </m:mrow>
                </m:mfrac>
            </m:math>
            <m:math display="block">
                <m:mi>x</m:mi>
                <m:mo>=</m:mo>
                <m:mi>a</m:mi>
                <m:mo>+</m:mo>
                <m:mfrac bevelled="true">
                    <m:mi>b</m:mi>
                    <m:mi>c</m:mi>
                </m:mfrac>
                <m:mo>+</m:mo>
                <m:mi>d</m:mi>
            </m:math>
            <m:math display="block">
                <m:mi>x</m:mi>
                <m:mo>=</m:mo>
                <m:mfrac bevelled="true">
                    <m:mfenced>
                        <m:mrow><m:mi>a</m:mi>
                            <m:mo>+</m:mo>
                            <m:mi>b</m:mi>
                        </m:mrow>
                    </m:mfenced>
                    <m:mfenced>
                        <m:mrow>
                            <m:mi>c</m:mi>
                            <m:mo>+</m:mo>
                            <m:mi>d</m:mi>
                        </m:mrow>
                    </m:mfenced>
                </m:mfrac>
            </m:math>
        </x:context>

