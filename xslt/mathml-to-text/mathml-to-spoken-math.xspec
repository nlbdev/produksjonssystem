    <x:scenario label="Aritmetikk">
        <x:context>
            <body>
                <h2>Addisjon</h2>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mn>2</mn>
                    <mo>+</mo>
                    <mn>3</mn>
                    <mo>=</mo>
                    <mn>5</mn>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow>
                        <mn>2</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>x</mi>
                    </mrow>
                    <mo>+</mo>
                    <mi>x</mi>
                    <mo>+</mo>
                    <mi>a</mi>
                    <mo>=</mo>
                    <mrow>
                        <mn>3</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>x</mi>
                    </mrow>
                    <mo>+</mo>
                    <mi>a</mi>
                </math>
                <h2>Subtraksjon</h2>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mn>2</mn>
                    <mo>-</mo>
                    <mn>3</mn>
                    <mo>=</mo>
                    <mo>-</mo>
                    <mn>5</mn>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow>
                        <mo>-</mo>
                        <mn>2</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>x</mi>
                    </mrow>
                    <mo>+</mo>
                    <mi>x</mi>
                    <mo>-</mo>
                    <mi>a</mi>
                    <mo>=</mo>
                    <mo>-</mo>
                    <mi>x</mi>
                    <mo>-</mo>
                    <mi>a</mi>
                </math>
                <h3>Multiplikasjon</h3>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow>
                        <mn>2</mn>
                        <mo>&sdot;</mo>
                        <mn>3</mn>
                        <mo>=</mo>
                        <mn>6</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow>
                        <mo>-</mo>
                        <mn>2</mn>
                        <mo>&sdot;</mo>
                        <mn>3</mn>
                        <mo>=</mo>
                        <mo>-</mo>
                        <mn>6</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow>
                        <mn>2</mn>
                        <mo>&sdot;</mo>
                        <mi>x</mi>
                        <mo>+</mo>
                        <mn>5</mn>
                        <mo>&sdot;</mo>
                        <mi>x</mi>
                        <mo>-</mo>
                        <mn>4</mn>
                        <mo>&sdot;</mo>
                        <mi>a</mi>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mi>x</mi>
                        <mo>&InvisibleTimes;</mo>
                        <mfenced>
                            <mrow>
                                <mi>y</mi>
                                <mo>+</mo>
                                <mn>4</mn>
                            </mrow>
                        </mfenced>
                        <mo>=</mo>
                        <mi>x</mi>
                        <mo>&InvisibleTimes;</mo>
                        <mi>y</mi>
                        <mo>+</mo>
                        <mn>4</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>x</mi>
                    </mrow>
                </math>
                <h2>Divisjon</h2>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>8</mn>
                        <mo>:</mo>
                        <mn>2</mn>
                        <mo>=</mo>
                        <mn>4</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>4</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>y</mi>
                        <mo>:</mo>
                        <mi>y</mi>
                        <mo>=</mo>
                        <mn>4</mn>
                    </mrow>
                </math>
                <h2>Likhet og ulikhet</h2>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>10</mn>
                        <mo>=</mo>
                        <mn>10</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>9</mn>
                        <mo>&#x2260;</mo>
                        <mn>10</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>11</mn>
                        <mo>&gt;</mo>
                        <mn>10</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>1000</mn>
                        <mo>&#x226B;</mo>
                        <mn>0</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mi>y</mi>
                        <mo>&ge;</mo>
                        <mi>a</mi>
                        <mo>+</mo>
                        <mn>5</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow> 
                        <mn>9</mn>
                        <mo>&lt;</mo>
                        <mi>10</mi>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mrow>
                        <mo>-</mo>
                        <mn>1000</mn>
                        <mo>&#x226A;</mo>
                        <mn>0</mn>
                    </mrow>
                </math>
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                    <mi>x</mi>
                    <mo>&le;</mo>
                    <mn>2</mn>
                    <mo>&InvisibleTimes;</mo>
                    <mi>a</mi>
                </math>
            </body>
        </x:context>
        
        <x:expect label="Forventet">
            <body xmlns="http://www.w3.org/1999/xhtml" xmlns:m="http://www.w3.org/1998/Math/MathML">
            </body>
