    <x:scenario label="Tvetydige uttrykk">
        <x:context>
            <h1 class="title">i og j</h1>
            <p>Matematikere og fysikere bruker  <em>i</em> som symbol for den imaginære enhet: 
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="inline"><mi>&ImaginaryI;</mi><mo>=</mo><msqrt><mo>-</mo><mn>1</mn></msqrt></math>
            </p>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <mrow>
                    <msup>
                        <mi>e</mi>
                        <mrow>
                            <mi>&ImaginaryI;</mi>
                            <mo>&sdot;</mo>
                            <mi>&pi;</mi>
                        </mrow>
                    </msup>
                    <mo>+</mo>
                    <mn>1</mn>
                    <mo>=</mo>
                    <mn>0</mn>
                </mrow>
            </math>
            
            <p>Ingeniører bruker  <em>j</em> som symbol for den imaginære enhet: 
                <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="inline"><mi mathvariant="sans-serif-bold-italic">j</mi><mo>=</mo><msqrt><mo>-</mo><mn>1</mn></msqrt></math>
            </p>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <mrow>
                    <msup>
                        <mi>e</mi>
                        <mrow>
                            <mi mathvariant="sans-serif-bold-italic">j</mi>
                            <mo>&sdot;</mo>
                            <mi>&pi;</mi>
                        </mrow>
                    </msup>
                    <mo>+</mo>
                    <mn>1</mn>
                    <mo>=</mo>
                    <mn>0</mn>
                </mrow>
            </math>
        </x:context>

