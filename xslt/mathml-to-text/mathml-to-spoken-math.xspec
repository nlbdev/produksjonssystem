    <x:scenario label="Kvadratrot og potens">
        <x:context>
            <h2>Kvadratrot</h2>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <mi>x</mi>
                <mo>=</mo>
                <msqrt>
                    <mn>2</mn>
                </msqrt>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msqrt>
                    <mn>2</mn>
                </msqrt>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msqrt>
                    <mrow>
                        <mn>2</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>a</mi>
                        <mo>+</mo>
                        <mi>b</mi>
                    </mrow>
                </msqrt>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msqrt>
                    <mn>2</mn>
                </msqrt>
                <mo>+</mo>
                <mi>b</mi>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msqrt>
                    <mi>a</mi>
                </msqrt>
                <mo>+</mo>
                <mi>b</mi>
                <mo>&ne;</mo>
                <msqrt>
                    <mrow>
                        <mi>a</mi>
                        <mo>+</mo>
                        <mi>b</mi>
                    </mrow>
                </msqrt>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msqrt>
                    <mrow>
                        <mi>a</mi>
                        <mo>&InvisibleTimes;</mo>
                        <mi>b</mi>
                    </mrow>
                </msqrt>
                <mo>=</mo>
                <msqrt>
                    <mi>a</mi>
                </msqrt>
                <mo>&sdot;</mo>
                <msqrt>
                    <mi>b</mi>
                </msqrt>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msqrt>
                    <mrow>
                        <mi>a</mi>
                        <mo>+</mo>
                        <mi>b</mi>
                    </mrow>
                </msqrt>
                <mo>&ne;</mo>
                <msqrt>
                    <mi>a</mi>
                </msqrt>
                <mo>+</mo>
                <msqrt>
                    <mi>b</mi>
                </msqrt>
            </math>
            <h2>Potensregning</h2>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msup>
                    <mi>x</mi>
                    <mn>2</mn>
                </msup>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msup>
                    <mi>x</mi>
                    <mi>y</mi>
                </msup>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msup>
                    <mi>x</mi>
                    <mrow>
                        <mi>n</mi>
                        <mo>+</mo>
                        <mn>1</mn>
                    </mrow>
                </msup>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msup>
                    <mi>x</mi>
                    <mrow>
                        <mn>2</mn>
                        <mo>&InvisibleTimes;</mo>
                        <mi>n</mi>
                    </mrow>
                </msup>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msup>
                    <mi>x</mi>
                    <msup>
                        <mi>a</mi>
                        <mi>b</mi>
                    </msup>
                </msup>
            </math>
            <math xmlns="http://www.w3.org/1998/Math/MathML" mode="display" display="block">
                <msup>
                    <mi>x</mi>
                    <msup>
                        <mi>a</mi>
                        <mi>b</mi>
                    </msup>
                </msup>
                <mo>+</mo>
                <mn>2</mn>
                <mo>&ne;</mo>
                <msup>
                    <mi>x</mi>
                    <msup>
                        <mi>a</mi>
                        <mrow>
                            <mi>b</mi>
                            <mo>+</mo>
                            <mn>2</mn>
                        </mrow>
                    </msup>
                </msup>
            </math>
        </x:context>

        <x:expect label="Forventet">
            <body xmlns="http://www.w3.org/1999/xhtml" xmlns:m="http://www.w3.org/1998/Math/MathML">
            </body>
        </x:expect>
